<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_order_form_inherit_qty_booking" model="ir.ui.view">
        <field name="name">sale.order.form.inherit.qty.booking</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="/form/sheet/notebook/page/field[@name='order_line']/tree/field[@name='product_uom_qty']" position="after">
                <field name="qty_booking"/>
            </xpath>
        </field>
    </record>
</odoo>
